<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$(SolutionDir)Build\common.props" />
  <PropertyGroup>
    <!-- common nuget configuration see common.props-->
    <VersionPrefix>1.2.1</VersionPrefix>
    <VersionSuffix></VersionSuffix>

    <Product>Mbc.Pcs.Net</Product>

  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Ensure.That" Version="8.0.0" />
    <PackageReference Include="Mbc.Common.Interface" Version="0.1.0" />
    <PackageReference Include="Mbc.Pcs.Net.TwinCat.Ads" Version="4.2.166" />
    <PackageReference Include="NLog" Version="4.5.10" />
    <PackageReference Include="Optional" Version="4.0.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Mbc.Ads.Mapper\Mbc.Ads.Mapper.csproj" />
    <ProjectReference Include="..\Mbc.Ads.Utils\Mbc.Ads.Utils.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Interop.TCEVENTLOGGERLib">
      <HintPath>..\Libs\Interop.TCEVENTLOGGERLib.dll</HintPath>
    </Reference>
    <Reference Include="Interop.TcEventLogProxyLib">
      <HintPath>..\Libs\Interop.TcEventLogProxyLib.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Command\CommandResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>CommandResources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Command\CommandResources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>CommandResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
